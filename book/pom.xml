	<name>SemanticCMS² Book</name>
	<url>https://semanticcms.com/</url>
	<description>Documentation for SemanticCMS² in SemanticCMS format.</description>
	<inceptionYear>2016</inceptionYear>

	<scm>
		<connection>scm:git:git://github.com/aoindustries/semanticcms.git</connection>
		<developerConnection>scm:git:git@github.com:aoindustries/semanticcms.git</developerConnection>
		<url>https://github.com/aoindustries/semanticcms</url>
		<tag>HEAD</tag>
	</scm>

	<issueManagement>
		<system>GitHub Issues</system>
